{
  "name": "@miyauci/vue-data-table",
  "version": "1.0.0-beta.6",
  "main": "dist/vue-data-table.umd.js",
  "module": "dist/vue-data-table.es.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/vue-data-table.es.js",
      "require": "./dist/vue-data-table.umd.js"
    }
  },
  "scripts": {
    "commit": "yarn --cwd ../.. commit",
    "cz": "yarn commit",
    "dev": "vite",
    "lint:tsc": "vue-tsc",
    "build": "yarn build:lib && yarn build:types",
    "build:lib": "vite build",
    "build:types": "tsc src/index.ts --lib ESNEXT,DOM --outDir dist --types vite/client,./src/shims-vue --emitDeclarationOnly -d",
    "test:unit": "jest",
    "test": "yarn test:unit",
    "serve": "vite preview"
  },
  "dependencies": {
    "@miyauci/data-table-core": "1.0.0-beta.6",
    "rambda": "^6.7.0",
    "vue": "^3.0.9",
    "vue-i18n": "^9.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.9",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "vite": "^2.1.3",
    "vite-plugin-windicss": "^0.11.0",
    "vue-tsc": "^0.0.8"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
