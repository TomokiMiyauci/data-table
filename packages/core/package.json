    "build:lib": "rollup -c rollup.config.ts",
    "build:types": "dts-bundle-generator --no-banner --external-inlines=rambda -o dist/index.d.ts src/index.ts",
    "test": "echo test"
  },
  "devDependencies": {
    "rambda": "^6.7.0",
    "rollup": "^2.68.0",
    "rollup-plugin-commonjs": "^10.1.0",
